		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"eslint": "^8.49.0",
		"eslint-config-standard": "^17.1.0",
		"obsidian": "latest",
		"obsidian-typings": "^2.0.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
